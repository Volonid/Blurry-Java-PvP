{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.first_person": {
			"geometry": "geometry.first_person",
			"materials": [
				{
					"*": "variable.is_enchanted?material.enchanted:material.default"
				}
			],
			"textures": [
				"texture.default",
				"texture.enchanted"
			]
		},
		"controller.render.third_person": {
			"textures": [
				"texture.default",
				"texture.enchanted"
			],
			"materials": [
				{
					"*": "variable.is_enchanted?material.enchanted:material.default"
				}
			],
			"geometry": "geometry.third_person"
		},
		"controller.render.item_default_offhand": {
			"geometry": "geometry.offhand",
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"texture.default"
			]
		}
	}
}