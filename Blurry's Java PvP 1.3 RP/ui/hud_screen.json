{
	"test_panel": {
		"type": "panel",
		"anchor_from": "center",
		"anchor_to": "center",
		"size": [
			"100%",
			"100%cm + 0.5px"
		],
		"controls": [
			{
				"test@hud.test": {}
			}
		]
	},
	"test": {
		"type": "image",
		"anchor_to": "center",
		"anchor_from": "center",
		"texture": "textures/ui2/blank",
		"size": [
			"100%c + 10px",
			"100%cm + 7px"
		],
		"layer": 998,
		"bindings": [
			{
				"binding_name": "#text"
			},
			{
				"binding_type": "view",
				"source_property_name": "(('%.6s' * #text) = 'toast.')",
				"target_property_name": "#visible"
			},
			{
				"binding_type": "view",
				"source_property_name": "(#text - 'toast.')",
				"target_property_name": "#text_remainder"
			},
			{
				"binding_type": "view",
				"source_property_name": "(('%.200s' * #text_remainder) - '$')",
				"target_property_name": "#adv_text"
			},
			{
				"binding_type": "view",
				"source_property_name": "((#text_remainder - '$') - #adv_text)",
				"target_property_name": "#adv_icon"
			}
		],
		"controls": [
			{
				"text": {
					"type": "label",
					"font_scale_factor": 0.5,
					"layer": 999,
					"alpha": 0.5,
					"text": "#text",
					"anchor_to": "right_middle",
					"anchor_from": "right_middle",
					"offset": [
						"-6px",
						"8px"
					],
					"localize": false,
					"bindings": [
						{
							"binding_type": "view",
							"source_control_name": "test",
							"source_property_name": "#adv_text",
							"target_property_name": "#text",
							"resolve_sibling_scope": true
						},
						{
							"binding_type": "view",
							"source_property_name": "(not (#text = ''))",
							"target_property_name": "#visible"
						}
					]
				}
			},
			{
				"icon": {
					"type": "image",
					"layer": 1000,
					"size": [
						"24px",
						"7000px"
					],
					"anchor_from": "left_middle",
					"anchor_to": "left_middle",
					"offset": [
						"3px",
						"0%"
					],
					"keep_ratio": true,
					"texture": "#texture",
					"bindings": [
						{
							"binding_type": "view",
							"source_control_name": "test",
							"source_property_name": "#adv_icon",
							"target_property_name": "#texture",
							"resolve_sibling_scope": true
						},
						{
							"binding_type": "view",
							"binding_condition": "once",
							"source_property_name": "(not (#texture = ''))",
							"target_property_name": "#visible"
						}
					]
				}
			}
		]
	},
	"test_factory": {
		"type": "panel",
		"size": [
			"100%",
			"100%"
		],
		"factory": {
			"name": "chat_item_factory",
			"control_ids": {
				"chat_item": "test_panel@hud.test_panel"
			}
		}
	},
	"root_panel": {
		"modifications": [
			{
				"array_name": "controls",
				"operation": "insert_front",
				"value": [
					{
						"abcdave_factory3@hud.abcdave_factory3": {}
					}
				]
			},
			{
				"array_name": "controls",
				"operation": "insert_front",
				"value": [
					{
						"test_factory@hud.test_factory": {}
					}
				]
			},
			{
				"array_name": "controls",
				"operation": "insert_front",
				"value": [
					{
						"abcdave_factory@hud.abcdave_factory": {}
					}
				]
			},
			{
				"array_name": "controls",
				"operation": "insert_back",
				"value": [
					{
						"abcdave_factory2@hud.abcdave_factory2": {}
					}
				]
			}
		]
	},
	"chat_grid_item/chat_background": {
		"bindings": [
			{
				"binding_type": "view",
				"source_property_name": "(('§␛' + #text + '§r') - ('§␛' + 'toast.') = ('§␛' + #text + '§r'))",
				"target_property_name": "#visible"
			}
		]
	},
	"hud_actionbar_text": {
		"visible": "(('%.13s' * $actionbar_text) = 'notification.') = false",
		"$actionbar_text": "$actionbar_text"
	},
